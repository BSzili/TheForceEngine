#include "basic_enemy.dxs"

void spawnPowerup()
{
	// Spawn the key.
	Vec3f pos = DXL2_BiasTowardsPlayer(self.position, 0.1f);
	int objectId = DXL2_SpawnObject(pos, self.angles, self.sectorId, CLASS_FRAME, "IKEYR.FME");

	// Attach the appropriate logic to the new object.
	DXL2_AddLogic(objectId, LOGIC_RED);
}
